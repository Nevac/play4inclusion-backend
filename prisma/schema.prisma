generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by the Prisma Client.
model event_teilnehmer {
  id       Int      @id @default(autoincrement())
  event_id Int      @default(0)
  user_id  Int      @default(0)
  bezahlt  Int      @default(0)
  anwesend DateTime @default(dbgenerated("('0000-00-00 00:00:00')")) @db.DateTime(0)

  @@index([bezahlt], map: "bezahlt")
  @@index([event_id], map: "event_id")
  @@index([user_id], map: "user_id")
  @@ignore
}

model t_contest {
  tcid       Int      @id @default(autoincrement())
  tid        Int      @default(0)
  tcrunde    Int      @default(0)
  team_a     Int      @default(0)
  team_b     Int      @default(0)
  wins_a     Int      @default(0)
  wins_b     Int      @default(0)
  won        Int      @default(0)
  dateline   DateTime @default(dbgenerated("('0000-00-00 00:00:00')")) @db.DateTime(0)
  host       String   @default("") @db.VarChar(15)
  user_id    Int      @default(0)
  row        Int      @default(0)
  comments   Int      @default(0)
  starttime  DateTime @default(dbgenerated("('0000-00-00 00:00:00')")) @db.DateTime(0)
  ignoretime Int      @default(0)
  ready_a    DateTime @default(dbgenerated("('0000-00-00 00:00:00')")) @db.DateTime(0)
  ready_b    DateTime @default(dbgenerated("('0000-00-00 00:00:00')")) @db.DateTime(0)
  defaultwin Int      @default(0)
  intern     String?  @db.Text
  public     String?  @db.Text

  @@index([comments], map: "comments")
  @@index([row], map: "row")
  @@index([tcrunde], map: "tcrunde")
  @@index([team_a], map: "team_a")
  @@index([team_b], map: "team_b")
  @@index([tid], map: "tid")
  @@index([won], map: "won")
}

model t_teilnehmer {
  tnid        Int     @id @default(autoincrement())
  tid         Int     @default(0)
  tnname      String? @db.VarChar(255)
  tnanz       Int?
  tnleader    Int?
  tnpasswd    String? @db.VarChar(255)
  tcheckedin  Int     @default(0)
  loser       Int     @default(0)
  tnlogo      Int     @default(0)
  homepage    String? @db.VarChar(255)
  irc         String? @db.VarChar(255)
  description String? @db.Text
  countrycode String? @db.Char(2)
  rankpos     String  @default("") @db.VarChar(255)

  @@index([loser], map: "looser")
  @@index([tid], map: "tid")
  @@index([tnanz], map: "tnanz")
}

model t_teilnehmer_part {
  tnid     Int      @default(0)
  user_id  Int      @default(0)
  dateline DateTime @default(dbgenerated("('0000-00-00 00:00:00')")) @db.DateTime(0)

  @@id([tnid, user_id])
}

model user {
  id    Int    @id @default(autoincrement())
  nick  String @default("") @db.VarChar(50)
  email String @db.VarChar(255)

  @@index([email], map: "email")
  @@index([nick], map: "nick")
}

model t_turnier {
  tid                   Int      @id @default(autoincrement())
  tgroupid              Int      @default(0)
  tactive               Int      @default(0)
  topen                 Int      @default(0)
  tclosed               Int      @default(0)
  tpause                Int      @default(0)
  teventid              Int      @default(0)
  tname                 String   @default("") @db.VarChar(255)
  tplaytype             String   @default("") @db.VarChar(255)
  tparams               String   @default("") @db.VarChar(255)
  tminanz               Int?
  tmaxanz               Int?
  tuserproteam          Int      @default(0)
  tmoreplayer           Int      @default(0)
  tlogo                 String?  @db.VarChar(255)
  tregeln               String?  @db.Text
  tinfotext             String?  @db.Text
  tstart                DateTime @default(dbgenerated("('0000-00-00 00:00:00')")) @db.DateTime(0)
  troundtime            Int      @default(0)
  troundpause           Int      @default(0)
  tcheckin              Int      @default(0)
  tcheckintime          Int      @default(120)
  tautodefaultwin       Int      @default(0)
  tadultsonly           Int      @default(0)
  tpassword             String?  @db.VarChar(255)
  tprice1               String   @default("") @db.VarChar(255)
  tprice2               String   @default("") @db.VarChar(255)
  tprice3               String   @default("") @db.VarChar(255)
  tnight                Int      @default(1)
  tnightbegin           DateTime @default(dbgenerated("('03:00:00')")) @db.Time(0)
  tnightend             DateTime @default(dbgenerated("('10:00:00')")) @db.Time(0)
  tdefmap               String?  @db.Text
  tadmins               String?  @db.Text
  tcoins                Int      @default(0)
  tcoinsreturn          Int      @default(1)
  c_contests            Int?
  c_contests_complete   Int?
  c_teilnehmer          Int?
  c_teilnehmer_complete Int?
  c_complete            Int?
  c_round               String?  @db.VarChar(11)

  @@index([tactive], map: "tactive")
}
