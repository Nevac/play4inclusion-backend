  server_cpu  String   @default("") @db.VarChar(255)
  server_mhz  Int      @default(0)
  server_ram  Int      @default(0)
  server_hdd  Int      @default(0)
  ip          String   @default("") @db.VarChar(255)
  intern      String?  @db.Text
  dateline    DateTime @default(dbgenerated("('0000-00-00 00:00:00')")) @db.DateTime(0)

  @@index([event_id, user_id, active], map: "event_id")
}

model event_sitzgruppen {
  id        Int    @id @default(autoincrement())
  name      String @default("") @db.VarChar(255)
  pass      String @default("") @db.VarChar(255)
