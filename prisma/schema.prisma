  name             String   @default("") @db.VarChar(100)
  anzahl           Int      @default(0)
  preis            String?  @db.Text
  konto            String?  @db.Text
  kontocheck       DateTime @default(dbgenerated("('0000-00-00 00:00:00')")) @db.DateTime(0)
  begin            DateTime @default(dbgenerated("('0000-00-00 00:00:00')")) @db.DateTime(0)
  end              DateTime @default(dbgenerated("('0000-00-00 00:00:00')")) @db.DateTime(0)
  signupbegin      DateTime @default(dbgenerated("('0000-00-00 00:00:00')")) @db.DateTime(0)
  signupend        DateTime @default(dbgenerated("('0000-00-00 00:00:00')")) @db.DateTime(0)
  seatreservebegin DateTime @default(dbgenerated("('0000-00-00 00:00:00')")) @db.DateTime(0)
  seatreserveend   DateTime @default(dbgenerated("('0000-00-00 00:00:00')")) @db.DateTime(0)
  seatimage        Int      @default(0)
  location         String   @default("") @db.VarChar(50)
  voraussetzung    String?  @db.Text
  minalter         Int      @default(0)
  turnier          String?  @db.Text
  netzwerk         String?  @db.Text
  weiteres         String?  @db.Text
  banner           String   @default("") @db.VarChar(100)
  location_id      Int      @default(0)
  map_template     String   @default("default_de-at-ch") @db.VarChar(255)
  allow_signoff    Int      @default(1)
  canceled         DateTime @default(dbgenerated("('0000-00-00 00:00:00')")) @db.DateTime(0)
  lpde_update      Int      @default(0)
  lpde_partyid     Int      @default(0)
  lpde_email       String   @default("") @db.VarChar(255)
  lpde_pass        String   @default("") @db.VarChar(255)
  lpde_mfz         Int      @default(0)
  lpde_tickets     Int      @default(0)
  archiv_media     Int      @default(0)
  archiv_gallery   Int      @default(0)

  @@index([active], map: "active")
  @@index([begin], map: "begin")
  @@index([end], map: "end")
