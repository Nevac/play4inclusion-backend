generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["fullTextSearch", "fullTextIndex"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model article {
  id               Int      @id @default(autoincrement())
  section_id       Int      @default(0)
  dateline         DateTime @default(dbgenerated("('0000-00-00 00:00:00')")) @db.DateTime(0)
  user_id          Int      @default(0)
  generator_id     Int      @default(0)
  display_teamname Int      @default(0)
  title            String   @default("") @db.VarChar(255)
  content          String?  @db.VarChar(3000)
  encode           Int      @default(0)
  readonly         Int      @default(0)
  active           Int      @default(1)
  edited           DateTime @default(dbgenerated("('0000-00-00 00:00:00')")) @db.DateTime(0)
  edituser         Int      @default(0)
  count_read       Int      @default(1)
  allow_comments   Int      @default(0)
  comments         Int      @default(0)
  intern           String?  @db.Text

  @@index([content], map: "content")
  @@index([section_id], map: "section_id")
}

model article_section {
  id       Int     @id @default(autoincrement())
  name     String  @default("") @db.VarChar(255)
  short    String  @default("") @db.VarChar(255)
  urlshort String  @default("") @db.VarChar(255)
  intro    String? @db.Text
  template String  @default("") @db.VarChar(255)
}

model beamer_timeline {
  id            Int      @id @default(autoincrement())
  beamer_id     Int      @default(0)
  active        Int      @default(0)
  name          String   @default("") @db.VarChar(255)
  type          String   @default("") @db.VarChar(255)
  content       String?  @db.Text
  duration      Int      @default(10)
  transition    String   @default("") @db.VarChar(255)
  display_order Int      @default(0)
  dateline      DateTime @default(dbgenerated("('0000-00-00 00:00:00')")) @db.DateTime(0)
  template      String   @default("") @db.VarChar(255)

  @@index([active], map: "active")
  @@index([beamer_id], map: "beamer_id")
}

model beamer_view {
  id       Int    @id @default(autoincrement())
  active   Int    @default(0)
  name     String @default("") @db.VarChar(255)
  template String @default("beamer_page") @db.VarChar(255)

  @@index([active], map: "active")
}

model catering_basket {
  id         Int      @id @default(autoincrement())
  product_id Int      @default(0)
  user_id    Int      @default(0)
  quantity   Int      @default(0)
  time_added DateTime @default(dbgenerated("('0000-00-00 00:00:00')")) @db.DateTime(0)
}

model catering_groups {
  id          Int    @id @default(autoincrement())
  name        String @default("") @db.VarChar(255)
  active      Int    @default(1)
  allow_order Int    @default(1)

  @@index([active], map: "active")
}

model catering_konto {
  user_id Int   @id @default(0)
  credits Float @default(0.00)
}

model catering_order {
  id          Int      @id @default(autoincrement())
  user_id     Int      @default(0)
  final_price Decimal  @default(0.00) @db.Decimal(8, 2)
  price_paid  Decimal  @default(0.00) @db.Decimal(8, 2)
  time_added  DateTime @default(dbgenerated("('0000-00-00 00:00:00')")) @db.DateTime(0)
  status      Int      @default(0)
}

model catering_order_part {
  id           Int      @id @default(autoincrement())
  order_id     Int      @default(0)
  product_id   Int      @default(0)
  order_nr     String   @default("") @db.VarChar(50)
  name         String   @default("") @db.VarChar(255)
  quantity     Int      @default(0)
  price        Decimal  @default(0.00) @db.Decimal(8, 2)
  status       Int      @default(1)
  user_id      Int      @default(0)
  time_added   DateTime @default(dbgenerated("('0000-00-00 00:00:00')")) @db.DateTime(0)
  time_changed DateTime @default(dbgenerated("('0000-00-00 00:00:00')")) @db.DateTime(0)
}

model catering_products {
  id          Int     @id @default(autoincrement())
  order_nr    String  @default("") @db.VarChar(50)
  name        String  @default("") @db.VarChar(255)
  description String? @db.Text
  price       Decimal @default(0.00) @db.Decimal(8, 2)
  active      Int     @default(1)
  group_id    Int     @default(0)

  @@index([active], map: "active")
  @@index([group_id], map: "group_id")
}

model catering_transaction {
  id       Int      @id @default(autoincrement())
  konto_id Int      @default(0)
  user_id  Int      @default(0)
  credits  Float    @default(0.00)
  short    String   @default("") @db.VarChar(255)
  dateline DateTime @default(dbgenerated("('0000-00-00 00:00:00')")) @db.DateTime(0)

  @@index([konto_id], map: "user_id")
  @@index([user_id], map: "user_id_2")
}

model cms_media {
  id             Int              @id @default(autoincrement())
  parent_id      Int              @default(0)
  level          Int              @default(0)
  children       Int              @default(0)
  is_dir         Int              @default(0)
  refer_id       Int              @default(0)
  description    String           @default("") @db.VarChar(255)
  additional     String?          @db.Text
  filename       String           @default("") @db.VarChar(255)
  counter        Int              @default(0)
  user_id        Int              @default(0)
  readonly       Int              @default(0)
  dateline       DateTime         @default(dbgenerated("('0000-00-00 00:00:00')")) @db.DateTime(0)
  ip             String           @default("") @db.VarChar(15)
  dns            String           @default("") @db.VarChar(50)
  filesavename   String           @default("") @db.VarChar(255)
  filesize       Int              @default(0)
  filetype       String           @default("") @db.VarChar(255)
  imageinfo      String           @default("") @db.VarChar(255)
  access         cms_media_access @default(public)
  allow_comments Int              @default(0)
  comments       Int              @default(0)

  @@index([is_dir], map: "is_dir")
  @@index([parent_id], map: "parent_id")
}

model country_code2name {
  code     String @id @default("") @db.Char(2)
  codelong String @default("") @db.Char(3)
  name     String @default("") @db.VarChar(255)
}

model country_ip2code {
  ip_from BigInt @default(0)
  ip_to   BigInt @default(0)
  code    String @default("") @db.Char(2)

  @@id([ip_from, ip_to])
}

model dotlan_system {
  key   String @id @default("") @db.VarChar(20)
  value String @default("") @db.VarChar(255)
}

model event_location {
  id             Int     @id @default(autoincrement())
  name           String  @default("") @db.VarChar(255)
  common         String? @db.Text
  plan           String? @db.Text
  zip            String  @default("") @db.VarChar(10)
  city           String  @default("") @db.VarChar(255)
  countrycode    String  @default("") @db.Char(2)
  street         String  @default("") @db.VarChar(255)
  common_encode  Int     @default(1)
  plan_encode    Int     @default(1)
