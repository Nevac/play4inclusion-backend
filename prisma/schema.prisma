  @@index([block_id], map: "block_id")
  @@index([leader_id], map: "leader_id")
}

model event_sitzplan {
  id           Int    @id @default(autoincrement())
  event_id     Int?   @default(0)
  active       Int    @default(0)
  rows         Int    @default(0)
  cols         Int    @default(0)
  dir          Int    @default(0)
  autonr       Int    @default(1)
  name         String @default("0") @db.VarChar(255)
  allow_groups Int    @default(0)

  @@index([active], map: "active")
  @@index([event_id], map: "event_id")
}

model event_sitzplatz {
  id     Int     @id @default(autoincrement())
  block  Int     @default(0)
  row    Int     @default(0)
  col    Int     @default(0)
  type   Int     @default(0)
  status Int     @default(0)
  grp    String? @db.VarChar(255)
  nr     Int?
  etid   Int?    @default(0)

  @@index([block], map: "block")
  @@index([status], map: "status")
  @@index([type], map: "type")
}

model event_status {
  id            Int    @id @default(autoincrement())
  status        String @default("") @db.VarChar(50)
  status_extern String @default("") @db.VarChar(50)
  bezahlt       Int    @default(0)
}

model event_teilnehmer {
  id               Int      @id @default(autoincrement())
  event_id         Int      @default(0)
  user_id          Int      @default(0)
  user_status      Int      @default(0)
  bezahlt          Int      @default(0)
  zahl_typ         Int      @default(1)
  sitz_nr          String   @default("") @db.VarChar(50)
  sitz_id          Int      @default(0)
  sitz_block       Int      @default(0)
  anwesend         DateTime @default(dbgenerated("('0000-00-00 00:00:00')")) @db.DateTime(0)
  ip               String   @default("") @db.VarChar(40)
  joinevent        Int?
  payday           Int      @default(0)
  group_id         Int      @default(0)
  group_sort_order Int      @default(0)
  comment          String?  @db.Text

  @@index([bezahlt], map: "bezahlt")
  @@index([event_id], map: "event_id")
  @@index([group_id], map: "group_id")
  @@index([user_id], map: "user_id")
  @@index([zahl_typ], map: "zahl_typ")
}

model event_voucher {
  key               String   @id @db.VarChar(255)
  event_id          Int
  zahl_typ          Int      @default(10)
  category          String   @db.VarChar(255)
  activation_date   DateTime @default(dbgenerated("('0000-00-00 00:00:00')")) @db.DateTime(0)
  expiry_date       DateTime @default(dbgenerated("('0000-00-00 00:00:00')")) @db.DateTime(0)
  generated_date    DateTime @default(dbgenerated("('0000-00-00 00:00:00')")) @db.DateTime(0)
  generated_user_id Int
  used_date         DateTime @default(dbgenerated("('0000-00-00 00:00:00')")) @db.DateTime(0)
  used_user_id      Int?

  @@index([event_id, category], map: "event2category")
  @@index([event_id], map: "event_id")
  @@index([used_date], map: "used_date")
}

model events {
