model forum_cat {
  catid        Int    @id @default(autoincrement())
  title        String @default("") @db.VarChar(50)
  description  String @default("") @db.VarChar(255)
  displayorder Int    @default(0)
  visible      Int    @default(1)
}

model forum_files {
  id           Int      @id @default(autoincrement())
  filename     String   @default("") @db.VarChar(255)
  filetype     String   @default("") @db.VarChar(255)
  filesize     String   @default("") @db.VarChar(11)
  filesavename String   @default("") @db.VarChar(255)
  dateline     DateTime @default(dbgenerated("('0000-00-00 00:00:00')")) @db.DateTime(0)
  user_id      Int      @default(0)
}

model forum_main {
  forumid      Int       @id @default(autoincrement()) @db.UnsignedSmallInt
  catid        Int       @default(0)
  title        String    @default("") @db.VarChar(100)
  description  String    @default("") @db.VarChar(250)
  active       Int       @default(0) @db.SmallInt
  closed       Int       @default(0)
  show_latest  Int       @default(1)
  displayorder Int       @default(0) @db.SmallInt
  lastposttime DateTime? @default(dbgenerated("('0000-00-00 00:00:00')")) @db.DateTime(0)
  lastposter   String    @default("") @db.VarChar(50)
  threads      Int       @default(0)
  posts        Int       @default(0)
  moderators   String?   @db.Text
  private      Int       @default(0)
  priv_access  String?   @db.Text

  @@index([active], map: "active")
  @@index([catid], map: "catid")
  @@index([private], map: "private")
}

model forum_post {
  postid        Int      @id @default(autoincrement()) @db.UnsignedInt
  threadid      Int      @default(0) @db.UnsignedInt
  userid        Int      @default(0) @db.UnsignedInt
  dateline      DateTime @default(dbgenerated("('0000-00-00 00:00:00')")) @db.DateTime(0)
  pagetext      String?  @db.VarChar(3000)
  htmltext      String?  @db.Text
  remoteip      String   @default("") @db.VarChar(16)
  dnsname       String?  @db.VarChar(50)
  locked        Int      @default(0)
  invisible     Int      @default(0)
  history       String?  @db.Text
  attachment_id Int      @default(0)
  notify        Int      @default(0)
  quoteid       Int      @default(0)
  edited        DateTime @default(dbgenerated("('0000-00-00 00:00:00')")) @db.DateTime(0)
  edit_user_id  Int      @default(0)

  @@index([attachment_id], map: "attachment_id")
  @@index([dateline], map: "dateline")
  @@index([invisible], map: "invisible")
  @@index([notify], map: "notify")
  @@index([pagetext], map: "pagetext")
  @@index([threadid], map: "threadid")
  @@index([userid], map: "userid")
}

model forum_thread {
  threadid     Int              @id @default(autoincrement()) @db.UnsignedInt
  title        String           @default("") @db.VarChar(100)
  forumid      Int              @default(0) @db.UnsignedSmallInt
  closed       Int              @default(0)
  invisible    Int              @default(0)
  show_latest  Int              @default(1)
  sticky       Int              @default(0)
  user_id      Int              @default(0)
  firstposter  String           @default("") @db.VarChar(50)
  lastposter   String           @default("") @db.VarChar(50)
  lastposttime DateTime         @default(dbgenerated("('0000-00-00 00:00:00')")) @db.DateTime(0)
  replies      Int              @default(0)
  hits         Int              @default(0)
  fv_id        Int              @default(0)
  ext          forum_thread_ext @default(EMPTY_ENUM_VALUE)
  ext_id       Int              @default(0)

  @@index([ext], map: "ext")
  @@index([ext_id], map: "ext_id")
  @@index([forumid], map: "forumid")
  @@index([invisible], map: "invisible")
  @@index([lastposttime], map: "lastposttime")
  @@index([sticky], map: "sticky")
  @@index([user_id], map: "user_id")
}

model forum_visit_forum {
